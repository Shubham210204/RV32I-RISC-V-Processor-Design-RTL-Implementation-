#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5b36dfcb34d0 .scope module, "program_counter_tb" "program_counter_tb" 2 1;
 .timescale 0 0;
v0x5b36dfcda550_0 .var "clk", 0 0;
v0x5b36dfcda610_0 .net "pc", 31 0, v0x5b36dfcda210_0;  1 drivers
v0x5b36dfcda6e0_0 .var "pc_next", 31 0;
v0x5b36dfcda7e0_0 .var "reset", 0 0;
S_0x5b36dfcc5f90 .scope module, "uut" "program_counter" 2 6, 3 1 0, S_0x5b36dfcb34d0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 32 "pc_next";
    .port_info 3 /OUTPUT 32 "pc";
v0x5b36dfcc61b0_0 .net "clk", 0 0, v0x5b36dfcda550_0;  1 drivers
v0x5b36dfcda210_0 .var "pc", 31 0;
v0x5b36dfcda2f0_0 .net "pc_next", 31 0, v0x5b36dfcda6e0_0;  1 drivers
v0x5b36dfcda3e0_0 .net "reset", 0 0, v0x5b36dfcda7e0_0;  1 drivers
E_0x5b36dfcc5500 .event posedge, v0x5b36dfcda3e0_0, v0x5b36dfcc61b0_0;
    .scope S_0x5b36dfcc5f90;
T_0 ;
    %wait E_0x5b36dfcc5500;
    %load/vec4 v0x5b36dfcda3e0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x5b36dfcda210_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5b36dfcda2f0_0;
    %assign/vec4 v0x5b36dfcda210_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5b36dfcb34d0;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x5b36dfcda550_0;
    %inv;
    %store/vec4 v0x5b36dfcda550_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5b36dfcb34d0;
T_2 ;
    %vpi_call 2 11 "$dumpfile", "program_counter.vcd" {0 0 0};
    %vpi_call 2 12 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5b36dfcb34d0 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5b36dfcb34d0;
T_3 ;
    %vpi_call 2 16 "$display", "Time\011Reset\011PC_Next\011\011PC" {0 0 0};
    %vpi_call 2 17 "$monitor", "%0d\011%b\011%h\011%h", $time, v0x5b36dfcda7e0_0, v0x5b36dfcda6e0_0, v0x5b36dfcda610_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b36dfcda550_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b36dfcda7e0_0, 0, 1;
    %pushi/vec4 4, 0, 32;
    %store/vec4 v0x5b36dfcda6e0_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b36dfcda7e0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 8, 0, 32;
    %store/vec4 v0x5b36dfcda6e0_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 12, 0, 32;
    %store/vec4 v0x5b36dfcda6e0_0, 0, 32;
    %delay 10, 0;
    %vpi_call 2 22 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
